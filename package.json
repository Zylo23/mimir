{
    "name": "mimir-db",
    "version": "0.0.0",
    "description": "Mimir DB is a simple in-memory database library for JavaScript/TypeScript applications.",
    "license": "MIT",
    "author": "Zylo23 <pyyupsk@proton.me>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Zylo23/mimir-db.git"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "format:fix": "prettier --write . --list-different",
        "postinstall": "husky install",
        "knip": "knip",
        "knip:fix": "knip --fix",
        "lint": "eslint . --ext .ts",
        "prepare": "husky",
        "publish:local": "yalc publish",
        "publish:npm": "changeset publish",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.3",
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "knip": "^5.17.2",
        "lint-staged": "^15.2.5",
        "prettier": "^3.2.5",
        "prettier-plugin-package-perfection": "^1.1.0",
        "prettier-plugin-sort-json": "^4.0.0",
        "ts-jest": "^29.1.4",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5",
        "validate-branch-name": "^1.3.0",
        "yalc": "1.0.0-pre.53"
    },
    "keywords": [
        "database",
        "in-memory database",
        "javascript",
        "library",
        "typescript"
    ]
}